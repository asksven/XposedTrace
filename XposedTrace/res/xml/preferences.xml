            android:defaultValue="true"
            android:enabled="true"
            android:key="trace_alarm_manager"
            android:summary="@string/trace_alarm_manager_summary"
            android:title="@string/trace_alarm_manager_title" />
        <CheckBoxPreference
            android:defaultValue="true"
