            android:key="trace_alarm_manager_service"
            android:summary="@string/trace_alarm_manager_service_summary"
            android:title="@string/trace_alarm_manager_service_title" />
        <CheckBoxPreference
            android:defaultValue="true"
            android:enabled="true"
            android:key="trace_location_manager"
            android:summary="@string/trace_location_manager_summary"
            android:title="@string/trace_location_manager_title" />
        <CheckBoxPreference
            android:defaultValue="true"
            android:enabled="true"
            android:key="trace_location_manager_service"
            android:summary="@string/trace_location_manager_service_summary"
            android:title="@string/trace_location_manager_service_title" />
        
        </PreferenceCategory>

</PreferenceScreen>